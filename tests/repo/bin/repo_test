#!/bin/sh

#
# repo_test script in sh for the repository README.adoc and no main() function
. helper.sh

for arg do
  case "${arg}" in
    --desc|--help|--manrepo|--version) COMMAND="${0##*/}" parse-man "${arg}"; exit ;;
    --debug|--dry-run|--no-quiet|--quiet|--verbose|--warning|--white) parse "${arg}" ;;
  esac
done


