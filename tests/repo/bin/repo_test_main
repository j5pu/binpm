#!/bin/sh

. helper.sh

#######################################
# repo_test_main is a test script in sh with main() function and not file header
#######################################
main() {
  for arg do
    case "${arg}" in
      --desc|--help|--manrepo|--version) COMMAND="${0##*/}" parse-man "${arg}"; exit ;;
      --debug|--dry-run|--no-quiet|--quiet|--verbose|--warning|--white) parse "${arg}" ;;
    esac
  done
}

main "$@"
